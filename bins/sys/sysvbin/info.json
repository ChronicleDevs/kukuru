{
	"codename": "sysvbin",
	"name": "SYSVBIN System Binaries v1.0",
  "author": "Yokaa",
  "description": "The most essentials things you need.",
	"version": "v1.0",
	"cmd": {
		"y": {
			"bin": "(app_root)/bin/bin.py"
		},
		"x": {
			"bin": "(app_root)/bin/XXbin.py"
		},
		"cd": {
			"bin": "(app_root)/bin/cd.py"
		},
		"ls": {
			"bin": "(app_root)/bin/ls.py"
		},
		"pwd": {
			"bin": "(app_root)/bin/pwd.py"
		}
	},
	"deps": {
		"inline": {
			"sysv": "stable"
		}
	}
}
